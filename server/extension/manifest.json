{
  "name": "Extension Demo",
  "version": "1.0",
  "description": "Build an Basic Extension!",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "/pages/index.html",
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://requirejs.org/docs/release/2.3.5/minified/require.js; object-src 'self'"
}