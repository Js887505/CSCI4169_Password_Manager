{
  "name": "IM2 Password Manager",
  "version": "1.0",
  "description": "Iron Man 2 - High Fidelity Prototype Password Manager",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "/pages/index.html",
